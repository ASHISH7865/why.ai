{
  "name": "why.ai",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx --watch --env-file .env src/index.ts",
    "start": "node --env-file .env dist/src/index.js",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "docker:build": "docker build -t why-ai-backend .",
    "docker:run": "docker run -p 9001:9001 why-ai-backend",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f app"
  },
  "keywords": [
    "ai",
    "learning",
    "chat",
    "education"
  ],
  "author": "",
  "license": "ISC",
  "description": "Why.AI - First-principles learning platform with contextual exploration",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.18.0",
    "@google/generative-ai": "^0.21.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.52.7",
    "redis": "^4.6.13",
    "uuid": "^10.0.0",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.2.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.33.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  }
}
